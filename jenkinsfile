@Library('MyLibrary') _
pipeline {
    agent any
    stages {
        
        stage('Hello World') {
            steps {
                script 
                {
                    helloworld.hello()
                }
            }
        }
         stage('Checkout code from Git') {
            steps {
               
                parallel(
                "1": {dir("dir1"){script{checkout_git.checkout_git("simple-java-maven-app")}}}
                )
                        }
                  }                                
         stage('Trigger AWS Code Build') {
            steps {
                parallel(
                "1": {dir("dir1"){script{aws_codebuild.aws_codebuild("java-project2")}}}
                )
                        }
                  }                     

            }
        }
  

